<Window x:Class="PSPSync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PSPSync"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="890.175">
    <Grid>
        <ComboBox x:Name="StorageDevice1" HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top" Width="414" SelectionChanged="StorageDevice1_SelectionChanged"/>
        <ListBox x:Name="SD1s" HorizontalAlignment="Left" Height="333" Margin="10,76,0,0" VerticalAlignment="Top" Width="414"/>
        <Button x:Name="SD1toSD2" Content="&gt;" HorizontalAlignment="Left" Margin="432,151,0,0" VerticalAlignment="Top" Width="30" Click="SD1toSD2_Click"/>
        <Button x:Name="SD2toSD1" Content="&lt;" HorizontalAlignment="Left" Margin="432,222,0,0" VerticalAlignment="Top" Width="30" Click="SD2toSD1_Click"/>
        <ComboBox x:Name="StorageDevice2" HorizontalAlignment="Left" Margin="476,49,0,0" VerticalAlignment="Top" Width="396" SelectionChanged="StorageDevice2_SelectionChanged"/>
        <ListBox x:Name="SD2s" HorizontalAlignment="Left" Height="333" Margin="476,76,0,0" VerticalAlignment="Top" Width="396"/>
        <Button x:Name="Sync" Content="Sync" HorizontalAlignment="Left" Margin="432,291,0,0" VerticalAlignment="Top" Width="30" Click="Sync_Click"/>
        <Button x:Name="SD1Delete" Content="Delete" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top" Width="45" Click="SD1Delete_Click"/>
        <Button x:Name="SD2Delete" Content="Delete" HorizontalAlignment="Left" Margin="827,24,0,0" VerticalAlignment="Top" Width="45" Click="SD2Delete_Click"/>
        <Button x:Name="Config" Content="Settings" HorizontalAlignment="Left" Margin="413,10,0,0" VerticalAlignment="Top" Width="74" Click="Config_Click"/>

    </Grid>
</Window>
