<UserControl x:Class="PSPSync.SaveListItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PSPSync"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Image x:Name="GamePic" HorizontalAlignment="Left" Height="75" VerticalAlignment="Top" Width="136" Margin="0,13,0,0"/>
        <Label x:Name="GameName" Content="Game name" HorizontalAlignment="Left" Margin="141,0,0,0" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
        <Label x:Name="GameInfo" Content="info" HorizontalAlignment="Left" Margin="141,13,0,0" VerticalAlignment="Top" FontSize="11" Foreground="White"/>
        <Label x:Name="GameDate" Content="directory" HorizontalAlignment="Left" Margin="0,-7,0,0" VerticalAlignment="Top" FontSize="10" Foreground="White"/>
        <Label x:Name="GameInfo2" Content="info" HorizontalAlignment="Left" Margin="141,29,0,0" VerticalAlignment="Top" FontSize="11" Foreground="White"/>

    </Grid>
</UserControl>
